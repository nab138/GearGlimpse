{
  "name": "gear-glimpse",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test.e2e": "cypress run",
    "test.unit": "vitest",
    "lint": "eslint",
    "wv": "node -e \"let version = require('./src/version.json'); let parts = version.version.split('.'); parts[2] = parseInt(parts[2]) + 1; version.version = parts.join('.'); require('fs').writeFileSync('./src/version.json', JSON.stringify(version, null, 2));\"",
    "nv": "npm version patch --no-git-tag-version && node -e \"let fs = require('fs'); let buildNumber = parseInt(fs.readFileSync('./build-number.txt', 'utf8')) + 1; fs.writeFileSync('./build-number.txt', buildNumber.toString());\" && npx capacitor-set-version set:ios -v $(node -p \"require('./package.json').version\") -b $(node -p \"require('fs').readFileSync('./build-number.txt', 'utf8')\") ."
  },
  "dependencies": {
    "@awesome-cordova-plugins/core": "^6.4.0",
    "@awesome-cordova-plugins/screen-orientation": "^6.4.0",
    "@capacitor/app": "5.0.7",
    "@capacitor/core": "5.7.4",
    "@capacitor/haptics": "5.0.7",
    "@capacitor/ios": "5.7.4",
    "@capacitor/keyboard": "5.0.8",
    "@capacitor/live-updates": "^0.1.3",
    "@capacitor/status-bar": "5.0.7",
    "@ionic/react": "^7.0.0",
    "@ionic/react-router": "^7.0.0",
    "@ionic/storage": "^4.0.0",
    "@react-three/drei": "^9.102.6",
    "@react-three/fiber": "^8.16.0",
    "@robloche/chartjs-plugin-streaming": "^3.1.0",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "camera-controls": "^2.7.2",
    "chart.js": "^4.4.0",
    "chartjs-adapter-luxon": "^1.3.1",
    "chartjs-plugin-zoom": "^2.0.1",
    "cordova-plugin-screen-orientation": "^3.0.3",
    "es6-promise-plugin": "4.2.2",
    "ionicons": "^7.0.0",
    "luxon": "^3.4.3",
    "ntcore-ts-client-monorepo": "github:nab138/ntcore-ts-client",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "three": "^0.156.1"
  },
  "devDependencies": {
    "@capacitor/cli": "5.7.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/three": "^0.156.0",
    "@vitejs/plugin-legacy": "^4.0.2",
    "@vitejs/plugin-react": "^4.0.1",
    "cypress": "^12.7.0",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "jsdom": "^22.1.0",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  },
  "description": "iOS FRC Telemetry Application"
}
